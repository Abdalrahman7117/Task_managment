@{
}
<body>
    <table class="table table-hover table-danger">
        <thead>
            <tr>
                <th>id</th>
                <th>description</th>
                <th>Status</th>
                <th>Priority</th>
                <th>Deadline</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var items in Model)
            {
                <tr>
                    <td>@items.Id</td>
                    <td>@items.Description</td>
                    <td>@items.Status</td>
                    <td>@items.Priority</td>
                    <td>@items.Deadline</td>
                    <td>

                        <a asp-action="Edit" asp-controller="Task_Manage" asp-route-id="@items.Id" class="btn btn-primary">Edit_Status</a>
                        

                    </td>

                </tr>

            }


        </tbody>

    </table>

</body>
