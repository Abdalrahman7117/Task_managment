@model IEnumerable<Projects>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<body>
    <a class="btn btn-danger" href="/Project/Create">Create</a>
    <table class="table table-hover table-danger">
        <thead>
            <tr>
                <th>id</th>
                <th>name</th>
                <th>description</th>
                <th>startdate</th>
                <th>enddate</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var items in Model)
            {
                <tr>
                    <td>@items.Pr_Id</td>
                    <td>@items.Name</td>
                    <td>@items.Description</td>
                    <td>@items.Startdate</td>
                    <td>@items.Enddate</td>
                    <td>
                
                    <a asp-action="Update" asp-controller="Project" asp-route-id="@items.Pr_Id" class="btn btn-primary">Update</a>
                    <a class="btn btn-danger" asp-action="Delete" asp-controller="Project" asp-route-id="@items.Pr_Id">Delete</a>
                
                    </td>
                   
                </tr>
              
            }

          
        </tbody>

    </table>

</body>
